{
  "manifest_version": 2,
  "name": "Real Simple Subscriptions",
  "short_name": "RSS",
  "version": "0.0.4",
  "description": "An RSS reader fully managed and contained within Chrome",
  "icons": {
    "128": "icons/icon-128.png",
    "48": "icons/icon-48.png",
    "16": "icons/icon-16.png"
  },
  "browser_action": {
    "default_title": "RSS"
  },
  "background": {
    "page": "index.html",
    "persistent": false
  },
  "options_page"	: "index.html",
  "permissions": [
    "<all_urls>",
    "tabs",
    "alarms"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
